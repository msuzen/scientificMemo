   "source": [
    "plt.title('Bernoulli Trial 2-sites: Average on sites')\n",
    "plt.xlabel('Time Step')\n",
    "plt.ylabel('Time Averaged : Average on sites')\n",
    "plt.plot(_time_averaged_observable, label='Time-averaged')\n",
    "plt.axhline(y=0.5, color='r', linestyle='-', label='Ensemble-averaged')\n",
    "plt.legend()\n",
    "plt.text(0.1, 0.28,'(c) 2022 \\n Mehmet Süzen',\n",
